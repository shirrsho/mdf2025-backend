{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate backend code",
  "author": "Shirsho Amin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write --log-level warn \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy": "cp -r src/modules/shared/mailsender/templates/ dist/modules/shared/mailsender/templates/",
    "poststart": "pnpm run copy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "generate": "node generateModule.js",
    "prepare": "husky",
    "doc:start": "npx compodoc -s ",
    "doc:build": "npx compodoc -p tsconfig.json"
  },
